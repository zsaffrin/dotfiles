# Shell navigation aliases
alias l="eza --long --color=always --icons=always --no-time --no-user --no-permissions"
alias la="eza --all --long --color=always --icons=always --no-time --no-user --no-permissions"
alias lt="eza --tree --icons=always"
alias lta="eza --all --tree --icons=always"

# Docker aliases
alias dcup="docker compose up -d"
alias dcupl="docker compose up -d && docker compose logs -tf"
alias dcdown="docker compose down"